{
  "name": "buzzin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite dev",
    "backend:up": "docker compose up -d",
    "backend:down": "docker compose down",
    "backend:clear": "docker compose down -v",
    "start": "node .output/server/index.mjs",
    "build": "vite build",
    "migrate": "drizzle-kit migrate",
    "migrate:generate": "drizzle-kit generate",
    "psql": "dotenvx run -- sh -c 'psql \"$DATABASE_URL\"'",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "vitest run --config vitest.config.ts tests/",
    "lint:check": "eslint .",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "CF_PAGES=1 pnpm build && wrangler pages deploy .output/public",
    "deploy:preview": "CF_PAGES=1 pnpm build && wrangler pages deploy .output/public --branch preview"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.1",
    "@electric-sql/client": "^1.2.0",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/electric-db-collection": "^0.2.8",
    "@tanstack/react-db": "^0.1.52",
    "@tanstack/react-router": "^1.139.7",
    "@tanstack/react-router-devtools": "^1.139.7",
    "@tanstack/react-start": "^1.139.9",
    "@tanstack/router-plugin": "^1.139.7",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "ai": "^6.0.3",
    "better-auth": "^1.4.3",
    "canvas-confetti": "^1.9.4",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "lucide-react": "^0.562.0",
    "nitro": "latest",
    "pg": "^8.16.3",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.13",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@tanstack/devtools-vite": "^0.4.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.2.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^22.10.2",
    "@types/pg": "^8.10.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "jsdom": "^27.0.0",
    "open-cli": "^8.0.0",
    "prettier": "^3.6.2",
    "wrangler": "^4.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.15",
    "web-vitals": "^5.1.0"
  }
}
